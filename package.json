    "url": "git+https://github.com/mikesamuel/mintable.git"
  },
  "bugs": {
    "url": "https://github.com/mikesamuel/mintable/issues"
  },
  "devDependencies": {
    "chai": ">=4.1.2",
    "eslint": ">=4.15.0",
    "eslint-config-strict": "*",
    "eslint-config-standard": "*",
    "mocha": ">=4.0.1",
    "standard": "*"
  },
  "eslintConfig": {
    "extends": [
